                'test' ~* '^te.*' as line_4, 'test' ~* '^tE.*', 'test' ~* 'te.*', 'test' ~* 'es', 'test' ~* '^es$',
                'test' !~* '^te.*' as line_5, 'test' !~* '^tE.*', 'test' !~* 'te.*', 'test' !~* 'es', 'test' !~* '^es$',
