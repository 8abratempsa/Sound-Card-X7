octosql "SELECT 'test' LIKE '%es%' as line_1, 'test' LIKE 'es', 'test' LIKE 'tes%', 'test' LIKE '%est', 'test' LIKE 'es%', 'test' LIKE 't_st', 'test' LIKE 't_t',
                'test' ~ '^te.*' as line_2, 'test' ~ '^tE.*', 'test' ~ 'te.*', 'test' ~ 'es', 'test' ~ '^es$',
